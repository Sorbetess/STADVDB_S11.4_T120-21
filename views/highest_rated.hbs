<div class="container">
  <h2>{{ title }}</h2>
  <br>

  <div id="form">

    <form name="year" id="year" method="post">

      <label for="year">Year:</label>

      <select id="year" name="year">

        {{#each years}}
          <option value={{year}}>{{year}}</option>
        {{/each}}

      </select>
      <input type="hidden" name = "page" value = '1'>

      <button onclick="submit()">Search</button>
    </form>

  </div>

</div>