<div class="container">
  <h2>{{ title }}</h2>
  <br>
  <div class="row">
    <div class="title col-1">
    </div>
    <div class="title col-6">
      <p>Title</p>
    </div>
    <div class="title col-5">
      <p>Net income</p>
    </div>
  </div>

  {{#each movies}}
  <div class="row">
    <div class="title center col-1">
      <p>{{inc @index}}</p>
    </div>
    <div class="col-6">
      <div class="title">{{title}}</div>
      <div class="timestamp">
        <p>{{release_date}}</p>
      </div>
    </div>
    <div class="col-5">
      <p>{{net_income}}</p>
    </div>
  </div>
  <hr>
  {{/each}}

</div>