<div class="container">
  <h2>{{ title }}</h2>
  <br>

  <div class="row">
      <div class="title col-5">
        <p>Month</p>
      </div>
      <div class="title col-5">
        <p>Genre</p>
      </div>
      <div class="title col-5">
        <p>Average Revenue</p>
      </div>
  </div>

  {{#each results}}
  <div class="row">
    <div class="title col-5">
        <p>{{month}}</p>
      </div>
    <div class="col-5">
      <div class="title">
        <p>{{genre}}</p>
      </div>
    </div>
    <div class="col-5">
      <p>{{tomoney avg_revenue}}</p>
    </div>
  </div>
  <hr>
  {{/each}}

</div>