      <p>Month</p>
    </div>
    <div class="title col-6">
      <p>Genre</p>
    </div>
    <div class="title col-4">
      <p>Average Revenue</p>
    </div>
  </div>

  {{#each results}}
  <div class="row">
    <div class="title2 col-2">
      <p>{{month}}</p>
    </div>
    <div class="col-6">
      <div class="title2">
        <p>{{genre}}</p>
      </div>
    </div>
    <div class="col-4">
      <p>{{tomoney avg_revenue}}</p>
    </div>
  </div>
  <hr>
  {{/each}}

</div>