<div class="container">
  <h2>{{ title }}</h2>
  <br>

  <div class="row">
    <div class="title col-2">
      <p>Month</p>
    </div>
    <div class="title col-5">
      <p>Company</p>
    </div>
    <div class="title col-5">
      <p>Average Revenue</p>
    </div>
  </div>

  {{#each results}}
  <div class="row">
    <div class="title col-2">
      <p>{{month}}</p>
    </div>
    <div class="col-5">
      <p class="title">{{company}}</p>
      <p>Movie: {{movie}}</p>
    </div>
    <div class="col-5">
      <p>{{tomoney avg_revenue}}</p>
    </div>
  </div>
  <hr>
  {{/each}}

  <div id={{offset}}></div>

</div>