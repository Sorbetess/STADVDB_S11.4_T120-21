<div class="container">
  <h2>{{ title }}</h2>
  <br>

  <div class="row">
      <div class="title col-1">
        <p>Quarter</p>
      </div>
      <div class="title col-6">
        <p>Company</p>
      </div>
      <div class="title col-5">
        <p>Movie</p>
      </div>
      <div class="title col-5">
        <p>Average Revenue</p>
      </div>
  </div>

  {{#each results}}
  <div class="row">
    <div class="title col-1">
        <p>{{quarter}}</p>
    </div>
    <div class="col-6">
      <div class="title">
        <p>{{company}}</p>
    </div>
    </div>
    <div class="col-5">
      <p>{{movie}}</p>
    </div>
    <div class="col-5">
      <p>{{tomoney avg_revenue}}</p>
    </div>
  </div>
  <hr>
  {{/each}}

</div>