<div class="container">
  <h2>{{ title }}</h2>
  <br>

  <div class="row">
    <div class="title col-2">
      <p>Quarter</p>
    </div>
    <div class="title col-6">
      <p>Company</p>
    </div>
    <div class="title col-4">
      <p>Average Revenue</p>
    </div>
  </div>

  {{#each results}}
  <div class="row">
    <div class="title col-2">
      <p>{{quarter}}</p>
    </div>
    <div class="col-6">
      <div class="title">
        {{company}}
      </div>
      <div class="timestamp">
        <p>{{movie}}</p>
      </div>
    </div>
    <div class="col-4">
      <p>{{tomoney avg_revenue}}</p>
    </div>
  </div>
  <hr>
  {{/each}}

</div>